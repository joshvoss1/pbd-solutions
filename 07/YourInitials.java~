public class YourInitials
{
        public static void (String[] args)
        {
        System.out.println("JJJJJ  V   V");
        System.out.println("  J    V   V");
        System.out.println("  J    V   V");
        System.out.println("  J    V   V");
        System.out.println("J J    V   V");
        System.out.println("J J     V V ");
        System.out.println("JJJ      V  ");
       }
       }
   
